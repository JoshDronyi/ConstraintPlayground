<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/constraintLayout"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">

    <!-- Begin  Constraint Guidelines-->


    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/gl_leftSide"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_begin="58dp" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/gl_rightSide"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_begin="369dp" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/gl_midBar"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_begin="313dp" />


    <!-- Begin  Left Side -->


    <Button
        android:id="@+id/btn_1"
        android:layout_width="50dp"
        android:layout_height="50dp"
        app:layout_constraintStart_toStartOf="@id/gl_leftSide"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/btn_2"
        android:layout_width="50dp"
        android:layout_height="50dp"
        app:layout_constraintStart_toStartOf="@id/gl_leftSide"
        app:layout_constraintTop_toBottomOf="@id/btn_1" />

    <Button
        android:id="@+id/btn_3"
        android:layout_width="50dp"
        android:layout_height="50dp"
        app:layout_constraintStart_toStartOf="@id/gl_leftSide"
        app:layout_constraintTop_toBottomOf="@id/btn_2" />

    <Button
        android:id="@+id/btn_4"
        android:layout_width="50dp"
        android:layout_height="50dp"
        app:layout_constraintStart_toStartOf="@id/gl_leftSide"
        app:layout_constraintTop_toBottomOf="@id/btn_3" />

    <Button
        android:id="@+id/btn_5"
        android:layout_width="50dp"
        android:layout_height="50dp"
        app:layout_constraintStart_toStartOf="@id/gl_leftSide"
        app:layout_constraintTop_toBottomOf="@id/btn_4" />

    <Button
        android:id="@+id/btn_6"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:text="@string/pivot"
        app:layout_constraintHorizontal_chainStyle="spread"
        app:layout_constraintBottom_toTopOf="@id/gl_midBar"
        app:layout_constraintStart_toStartOf="@id/gl_leftSide"
        app:layout_constraintTop_toBottomOf="@id/btn_5"
        app:layout_constraintEnd_toStartOf="@id/btn_32"/>

    <Button
        android:id="@+id/btn_7"
        android:layout_width="50dp"
        android:layout_height="50dp"
        app:layout_constraintStart_toStartOf="@id/gl_leftSide"
        app:layout_constraintTop_toBottomOf="@id/gl_midBar" />

    <Button
        android:id="@+id/btn_8"
        android:layout_width="50dp"
        android:layout_height="50dp"
        app:layout_constraintStart_toStartOf="@id/gl_leftSide"
        app:layout_constraintTop_toBottomOf="@id/btn_7" />

    <Button
        android:id="@+id/btn_9"
        android:layout_width="50dp"
        android:layout_height="50dp"
        app:layout_constraintStart_toStartOf="@id/gl_leftSide"
        app:layout_constraintTop_toBottomOf="@id/btn_8" />

    <Button
        android:id="@+id/btn_10"
        android:layout_width="50dp"
        android:layout_height="50dp"
        app:layout_constraintStart_toStartOf="@id/gl_leftSide"
        app:layout_constraintTop_toBottomOf="@id/btn_9" />

    <Button
        android:id="@+id/btn_11"
        android:layout_width="50dp"
        android:layout_height="50dp"
        app:layout_constraintStart_toStartOf="@id/gl_leftSide"
        app:layout_constraintTop_toBottomOf="@id/btn_10" />

    <Button
        android:id="@+id/btn_12"
        android:layout_width="50dp"
        android:layout_height="50dp"
        app:layout_constraintStart_toStartOf="@id/gl_leftSide"
        app:layout_constraintTop_toBottomOf="@id/btn_11" />

    <Button
        android:id="@+id/btn_13"
        android:layout_width="50dp"
        android:layout_height="50dp"
        app:layout_constraintStart_toStartOf="@id/gl_leftSide"
        app:layout_constraintTop_toBottomOf="@id/btn_12" />

    <Button
        android:id="@+id/btn_14"
        android:layout_width="50dp"
        android:layout_height="50dp"
        app:layout_constraintStart_toStartOf="@id/gl_leftSide"
        app:layout_constraintTop_toBottomOf="@id/btn_13" />

    <!-- Finish Left Side -->


    <!-- Begin  Right Side -->

    <Button
        android:id="@+id/btn_15"
        android:layout_width="50dp"
        android:layout_height="50dp"
        app:layout_constraintEnd_toEndOf="@id/gl_rightSide"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/btn_16"
        android:layout_width="50dp"
        android:layout_height="50dp"
        app:layout_constraintEnd_toEndOf="@id/gl_rightSide"
        app:layout_constraintTop_toBottomOf="@id/btn_15" />

    <Button
        android:id="@+id/btn_17"
        android:layout_width="50dp"
        android:layout_height="50dp"
        app:layout_constraintEnd_toEndOf="@id/gl_rightSide"
        app:layout_constraintTop_toBottomOf="@id/btn_16" />

    <Button
        android:id="@+id/btn_18"
        android:layout_width="50dp"
        android:layout_height="50dp"
        app:layout_constraintEnd_toEndOf="@id/gl_rightSide"
        app:layout_constraintTop_toBottomOf="@id/btn_17" />

    <Button
        android:id="@+id/btn_19"
        android:layout_width="50dp"
        android:layout_height="50dp"
        app:layout_constraintEnd_toEndOf="@id/gl_rightSide"
        app:layout_constraintTop_toBottomOf="@id/btn_18" />

    <Button
        android:id="@+id/btn_20"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:text="@string/pivot"
        app:layout_constraintBottom_toTopOf="@id/gl_midBar"
        app:layout_constraintEnd_toEndOf="@id/gl_rightSide"
        app:layout_constraintTop_toBottomOf="@id/btn_19"
        app:layout_constraintStart_toEndOf="@id/btn_29"/>

    <Button
        android:id="@+id/btn_21"
        android:layout_width="50dp"
        android:layout_height="50dp"
        app:layout_constraintEnd_toEndOf="@id/gl_rightSide"
        app:layout_constraintTop_toBottomOf="@id/gl_midBar" />

    <Button
        android:id="@+id/btn_22"
        android:layout_width="50dp"
        android:layout_height="50dp"
        app:layout_constraintEnd_toEndOf="@id/gl_rightSide"
        app:layout_constraintTop_toBottomOf="@id/btn_21" />

    <Button
        android:id="@+id/btn_23"
        android:layout_width="50dp"
        android:layout_height="50dp"
        app:layout_constraintEnd_toEndOf="@id/gl_rightSide"
        app:layout_constraintTop_toBottomOf="@id/btn_22" />

    <Button
        android:id="@+id/btn_24"
        android:layout_width="50dp"
        android:layout_height="50dp"
        app:layout_constraintEnd_toEndOf="@id/gl_rightSide"
        app:layout_constraintTop_toBottomOf="@id/btn_23" />

    <Button
        android:id="@+id/btn_25"
        android:layout_width="50dp"
        android:layout_height="50dp"
        app:layout_constraintEnd_toEndOf="@id/gl_rightSide"
        app:layout_constraintTop_toBottomOf="@id/btn_24" />

    <Button
        android:id="@+id/btn_26"
        android:layout_width="50dp"
        android:layout_height="50dp"
        app:layout_constraintEnd_toEndOf="@id/gl_rightSide"
        app:layout_constraintTop_toBottomOf="@id/btn_25" />

    <Button
        android:id="@+id/btn_27"
        android:layout_width="50dp"
        android:layout_height="50dp"
        app:layout_constraintEnd_toEndOf="@id/gl_rightSide"
        app:layout_constraintTop_toBottomOf="@id/btn_26" />

    <Button
        android:id="@+id/btn_28"
        android:layout_width="50dp"
        android:layout_height="50dp"
        app:layout_constraintEnd_toEndOf="@id/gl_rightSide"
        app:layout_constraintTop_toBottomOf="@id/btn_27" />
    <!-- Finish Left Side -->

    <!--Connecting Bar -->
    <Button
        android:id="@+id/btn_29"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:text="@string/pivot"
        app:layout_constraintBottom_toTopOf="@id/gl_midBar"
        app:layout_constraintEnd_toStartOf="@id/btn_20"
        app:layout_constraintTop_toBottomOf="@id/btn_19"
        app:layout_constraintStart_toEndOf="@id/btn_30"/>

    <Button
        android:id="@+id/btn_30"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:text="@string/pivot"
        app:layout_constraintBottom_toTopOf="@id/gl_midBar"
        app:layout_constraintEnd_toStartOf="@id/btn_29"
        app:layout_constraintTop_toBottomOf="@id/btn_19"
        app:layout_constraintStart_toEndOf="@id/btn_31"/>

    <Button
        android:id="@+id/btn_31"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:text="@string/pivot"
        app:layout_constraintBottom_toTopOf="@id/gl_midBar"
        app:layout_constraintEnd_toStartOf="@id/btn_30"
        app:layout_constraintTop_toBottomOf="@id/btn_19"
        app:layout_constraintStart_toEndOf="@id/btn_32"/>

    <Button
        android:id="@+id/btn_32"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:text="@string/pivot"
        app:layout_constraintBottom_toTopOf="@id/gl_midBar"
        app:layout_constraintEnd_toStartOf="@id/btn_31"
        app:layout_constraintStart_toEndOf="@id/btn_6"
        app:layout_constraintTop_toBottomOf="@id/btn_19" />


    <!-- End Connecting Bar-->
</androidx.constraintlayout.widget.ConstraintLayout>